l1=["apple","eagle","benz","fidget","chaitu"]
content="the actual list is {}"
print (content.format(l1))
appleindex=l1.index("apple")
print("index of apple is " + str(appleindex))
l1.insert(4,"dell")
for a in l1:
  print(a)
print("the list after sorting is as below.")
l1.sort()
for a in l1:
  print(a)
finalLength=len(l1)
print("length of the list =" + str(finalLength))
ecount=l1.count('apple')
print("e is present " + str(ecount) + " times.")
